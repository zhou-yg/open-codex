    "dist"
  ],
  "keywords": [
    "example",
    "typescript",
    "npm",
    "package",
    "sample",
    "boilerplate",
    "starter",
    "demo",
    "cjs",
    "esm",
    "umd"
  ],
  "license": "MIT",
